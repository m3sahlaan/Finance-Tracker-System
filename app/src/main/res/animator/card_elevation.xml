<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <objectAnimator
            android:propertyName="elevation"
            android:valueTo="16"
            android:duration="100" />
    </item>
    <item>
        <objectAnimator
            android:propertyName="elevation"
            android:valueTo="10"
            android:duration="100" />
    </item>
</selector>